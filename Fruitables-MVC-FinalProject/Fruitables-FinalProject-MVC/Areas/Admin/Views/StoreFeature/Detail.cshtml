@model StoreFeature

<div class="container mt-5">
    <form method="post" asp-action="Create" class="forms-sample">

        <div class="form-group">
            <label class="text-info">Icon Name: </label>
            <h3>@Model.IconName</h3>
        </div>

        <div class="form-group">
            <label class="text-info">Description: </label>
            <h3>@Model.Description</h3>
        </div>

        <div class="form-group">
            <label class="text-info">Feature: </label>
            <h3>@Model.Feature</h3>
        </div>

        <a asp-action="Index" class="btn btn-light">Back</a>
    </form>
</div> 